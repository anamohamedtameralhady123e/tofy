local allowedUsers = {
    ["TofyEvii"] = true,
    ["AnotherAllowedUser"] = true -- Ø£Ø¶Ù Ø£ÙŠ Ù…Ø³ØªØ®Ø¯Ù…ÙŠÙ† Ø¢Ø®Ø±ÙŠÙ† Ù‡Ù†Ø§
}

local player = game:GetService("Players").LocalPlayer
if not allowedUsers[player.Name] then
    game:GetService("Players").LocalPlayer:Kick("ðŸš« Ù‡Ø°Ø§ Ø§Ù„Ø³ÙƒØ±Ø¨Øª Ù…Ø­Ù…ÙŠ Ø¨ÙˆØ§Ø³Ø·Ø© Ø·ÙˆÙÙŠ Ø¥ÙŠÙÙŠ. Ù„ÙŠØ³ Ù„Ø¯ÙŠÙƒ ØµÙ„Ø§Ø­ÙŠØ© Ø§Ø³ØªØ®Ø¯Ø§Ù…Ù‡.")
